<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/dormDash"
        android:layout_width="382dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="32dp"
        android:text="dorm dash"
        app:layout_constraintBottom_toTopOf="@+id/diningHall"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/diningHall"
        android:layout_width="373dp"
        android:layout_height="56dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="329dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="100dp"
        android:text="diningHall"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dormDash" />

    <EditText
        android:id="@+id/welcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="99dp"
        android:layout_marginTop="147dp"
        android:layout_marginEnd="99dp"
        android:layout_marginBottom="177dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="Welcome."
        app:layout_constraintBottom_toTopOf="@+id/diningHall"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dormDash" />

    <EditText
        android:id="@+id/welcome2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="99dp"
        android:layout_marginEnd="99dp"
        android:layout_marginBottom="132dp"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        android:text="Select an Option to Begin."
        app:layout_constraintBottom_toTopOf="@+id/diningHall"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome" />

</androidx.constraintlayout.widget.ConstraintLayout>